# PriceDrop API Environment Variables

# Node Environment
NODE_ENV=production

# Database Configuration (Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supbase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supbase_service_key_here

# Email Configuration (SendGrid)
SENDGRID_API_KEY=your_sendgrid_api_key_here
FROM_EMAIL=noreply@yourdomain.com

# JWT Configuration
JWT_SECRET=your_jwt_secret_here

# RapidAPI Configuration
RAPIDAPI_HOST=pricedrop-api.p.rapidapi.com
RAPIDAPI_PROXY_SECRET=your_rapidapi_proxy_secret_here

# Rate Limiting
REDIS_URL=your_redis_url_here

# Monitoring
